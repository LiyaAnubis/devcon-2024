
@ВПодсистеме
метод ВОбъектТелеграм(): Соответствие<Строка, Объект?>
    
    знч ОбъектТелеграм = новый Соответствие<Строка, Объект?>()
    ОбъектТелеграм.Вставить("type", ТипСущности.ИдентификаторТелеграм())
    ОбъектТелеграм.Вставить("offset", Начало)
    ОбъектТелеграм.Вставить("length", Длина)
    
    если Пользователь != Неопределено
        ОбъектТелеграм.Вставить("user", УчетныеЗаписиТелеграм.ВОбъектТелеграм(Пользователь))
    ;

    возврат ОбъектТелеграм
;